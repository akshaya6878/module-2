#include<stdio.h>
int main()
{
    int numb,rem,sum=0,i;
    printf(" enter the number");
    scanf("%d",&numb);
    for(i=1;i<(numb);i++)
    {
        rem=numb%i;
        if(rem==0)
        {
            sum=sum+i;
        }
    }
    if (sum==numb)
    printf("perfect number");
    else
    printf("not a perfect number");
    return 0;
}
