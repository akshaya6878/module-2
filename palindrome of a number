#include<stdio.h>
int main()
{
    int n,o,r,rev=0;
    printf("enter the integer");
    scanf("%d",&n);
    o=n;
    while(n!=0){
        r=n%10;
        rev=rev*10+r;
        n/=10;
    }
    if(o=rev)
    printf("%d is palindrome",o);
    else
    printf("%d is not palindrome",o);
    return 0;
}
