#include<stdio.h>
int main()
{
    int i,j,rows;
    printf("enter the number of rows");
    scanf("%d",&rows);
    for(i=1;i<=rows;i++){
        for(j=1;j<=i;j++){
            printf("*");
        }
        printf("\n");
    }
    return 0;
}


#include<stdio.h>
int main()
{
    int i,j;
    for(i=1;j<=5;i++)
    {
        for(j=1;j<=i;j++)
    
    
    {
        printf("%c",'A'-1+i);
    }
    printf("\n");
    }
    return 0;
}


#include<stdio.h>
int main(){
    int i,j,k;
    for(i=1;i<=6;i++)
    {
        k=i;
        for(j=1;j<=i;j++,k++)
     {   printf("%d",k);
    
     }printf("\n");
     }
    return 0;
}
